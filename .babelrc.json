{
  "assumptions": {
    "noDocumentAll": true,
    "noClassCalls": true
  },
  "compact": true,
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "corejs": { "version": "3.8", "proposals": true },
        "useBuiltIns": "usage"
        // "debug": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        // "absoluteRuntime": false,
        "modules": false,
        "corejs": false
        // "helpers": true,
        // "regenerator": true,
        // "version": "^7.0.0",
      }
    ]
  ]
}
