{
  "assumptions": {
    "noDocumentAll": true,
    "noClassCalls": true
  },
  "compact": true,
  "sourceMaps": true,
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "corejs": { "version": 3, "proposals": true },
        // "debug": true,
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false
      }
    ]
  ]
}
